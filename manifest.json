{
   "background": {
      "scripts": [ "./js/background.js" ]
   },
   "browser_action": {
      "default_icon": "./img/icon.png",
      "default_popup": "./html/popup.html"
   },
   "description": "This simple extension allows you to track top coin on Uniswap.",
   "icons": {
      "128": "./img/icon128.png",
      "16": "./img/icon16.png",
      "48": "./img/icon48.png"
   },
   "manifest_version": 2,
   "name": "Top Uniswap Tracker",
   "options_ui": {
      "chrome_style": true,
      "page": "./html/welcome.html"
   },
   "permissions": [ "storage", "notifications", "contextMenus", "https://api.coingecko.com/api/v3/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1"
}
